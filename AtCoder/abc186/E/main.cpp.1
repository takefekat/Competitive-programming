#include <bits/stdc++.h>
using namespace std;
typedef long long ll;

signed main()
{
    ll T;
    cin >> T;

    for (ll i = 0; i < T; i++)
    {
        ll N, S, K;
        cin >> N >> S >> K;

        // (N-S) + N*i がKで割り切れるか？
        ll ans = -1;
        for (ll i = 0; i < 1000000000; i++)
        {
            if (((N - S) + (N * i)) % K == 0)
            {
                ans = ((N - S) + (N * i)) / K;
                break;
            }
        }
        cout << ans << endl;
    }
    return 0;
}
